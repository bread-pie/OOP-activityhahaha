using System;

class Program
{
    static void Main()
    {
        List<string> courses = new List<string>();

        Console.WriteLine("Register up to 6 courses (code must be 5 characters):");

        for (int i = 0; i < 6; i++)
        {
            Console.Write("Enter course code: ");
            string code = Console.ReadLine();

            if (courses.Contains(code))
            {
                Console.WriteLine("Error: Duplicate course code.");
                i--;
                continue;
            }

            if (code.Length != 5)
            {
                Console.WriteLine("Error: Code must be exactly 5 characters.");
                i--;
                continue;
            }

            courses.Add(code);
            Console.WriteLine("Course registered successfully.");
        }

        Console.WriteLine("\nRegistered courses:");
        foreach (string course in courses)
        {
            Console.WriteLine(course);
        }
    }
}
