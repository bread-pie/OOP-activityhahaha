using System;

class Program
{
    static void Main()
    {
        List<string> usernames = new List<string>();

        // Store 5 usernames
        Console.WriteLine("Enter 5 usernames:");
        for (int i = 0; i < 5; i++)
        {
            usernames.Add(Console.ReadLine());
        }

        // Ask for character to search
        Console.Write("\nEnter a character to search: ");
        char searchChar = Console.ReadLine().ToLower()[0]; // Take first character only

        Console.WriteLine("\nUsernames containing '" + searchChar + "':");
        bool found = false;

        foreach (string user in usernames)
        {
            if (user.ToLower().Contains(searchChar))
            {
                Console.WriteLine(user);
                found = true;
            }
        }

        if (!found)
        {
            Console.WriteLine("No usernames found.");
        }
    }
}
