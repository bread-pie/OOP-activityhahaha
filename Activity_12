using System;

class Program
{
    static void Main()
    {
        List<string> products = new List<string>();
        List<int> stocks = new List<int>();

        Console.WriteLine("Enter 10 products and their stock counts:");

        for (int i = 0; i < 10; i++)
        {
            Console.Write("Product name: ");
            products.Add(Console.ReadLine());

            Console.Write("Stock count: ");
            int stock = int.Parse(Console.ReadLine());

            // Restock if less than 10
            if (stock < 10)
            {
                stock += 20;
                Console.WriteLine("Restocked by +20 units.");
            }

            stocks.Add(stock);
        }

        Console.WriteLine("\nFinal Stock Levels:");
        for (int i = 0; i < products.Count; i++)
        {
            Console.WriteLine(products[i] + ": " + stocks[i]);
        }
    }
}
