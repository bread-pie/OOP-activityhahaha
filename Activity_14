using System;

class Program
{
    static void Main()
    {
        List<string> countries = new List<string>()
        {
            "Philippines", "Japan", "USA", "Canada", "Australia"
        };

        List<string> capitals = new List<string>()
        {
            "Manila", "Tokyo", "Washington", "Ottawa", "Canberra"
        };

        Console.Write("Enter a country name: ");
        string searchCountry = Console.ReadLine().ToLower();

        bool found = false;
        for (int i = 0; i < countries.Count; i++)
        {
            if (countries[i].ToLower() == searchCountry)
            {
                Console.WriteLine("Capital: " + capitals[i]);
                found = true;
                break;
            }
        }

        if (!found)
        {
            Console.WriteLine("Country not found.");
        }
    }
}
